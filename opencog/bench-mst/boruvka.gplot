
set term png small
set out 'boruvka.png'

set logscale x
set logscale y
set title "Time to parse"
set xlabel "Sentence Length"
set ylabel "Milliseconds"
plot "boruvka.dat" using 1:2 with lines linewidth 2 title "data", \
     "boruvka.dat" using 1:(0.002 * $1**2.8) with lines linewidth 2 title "fit"

